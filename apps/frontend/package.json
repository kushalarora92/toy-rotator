{
  "name": "app-frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start",
    "postinstall": "cd ../.. && pnpm --filter '@my-app/*' --filter '!app-frontend' build",
    "build:web": "expo export --platform web --output-dir web-build",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:prod": "eas build --platform ios --profile production",
    "update:preview": "eas update --channel preview --message",
    "update:production": "eas update --channel production --message"
  },
  "dependencies": {
    "@expo/html-elements": "^0.13.7",
    "@expo/vector-icons": "^15.0.2",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/core": "^3.0.10",
    "@gluestack-ui/themed": "^1.1.73",
    "@gluestack-ui/utils": "^3.0.7",
    "@legendapp/motion": "^2.4.0",
    "@my-app/types": "workspace:*",
    "@my-app/ui": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/analytics": "^23.4.0",
    "@react-native-firebase/app": "^23.4.0",
    "@react-native-firebase/auth": "^23.4.0",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.12",
    "expo-apple-authentication": "~8.0.8",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.8",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.8",
    "firebase": "^12.3.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-aria": "^3.44.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-stately": "^3.42.0",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}